{
    "mobley" : {
        "format" : "gromacs",
        "file" : "topgro",
        "mdft_output": "mdft_mmax1_ref.json",
        "values_to_parse" : {
            "calc" : {
                "label" : "MD Simulations"
            },
            "expt" : {
                "label" : "Experimental"
            }
        },
        "plots" : {
            "plotvs" : {
                "vs Experimental" : {
                    "x" : "expt",
                    "y" : "calc mdft_energy_pc mdft_energy_pc+"
                },
                "vs MD Simulations" : {
                    "x" : "calc",
                    "y" : "mdft_energy_pc mdft_energy_pc+"
                }
            },
            "errordistrib" : {
                "vs Experimental" : {
                    "x" : "expt",
                    "y" : "calc mdft_energy_pc mdft_energy_pc+",
                    "name" : "error_distribution_exp"
                },
                "vs MD Simulations" : {
                    "x" : "calc",
                    "y" : "mdft_energy_pc mdft_energy_pc+",
                    "name" : "error_distribution_calc"
                }
            },
            "errorby" : {
                "vs Experimental" : {
                    "x" : "expt",
                    "y" : "calc mdft_energy_pc",
                    "cat_column" : "groups"
                }
            },
            "unit" : "kcal/mol"
        }
    },
    "analogs" : {
        "format" : "json",
        "file" : "analog_mdft_db.json"
    }        
}
